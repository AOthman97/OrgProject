@model IEnumerable<ADminLteTest.Entites.OrgDetails>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Organization
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a asp-action="NewIndex" asp-route-OrgDetailsId="@item.Id">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
